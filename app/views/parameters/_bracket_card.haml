- values.each do |val|
  .card-body
    %ul.list-unstyled
      - val.each do |key, value|
        .card
          .card-header
            %li
              %h6= key.to_s.capitalize

          .card-body
            %ul
              - unless value.nil?
                - value.each do |k, v|
                  %p
                    %b= "#{k}:"
                    - v.each do |v|
                      = number_to_currency(v.last)
